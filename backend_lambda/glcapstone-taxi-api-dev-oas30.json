  "components": {
    "schemas": {
      "TaxiStatusLocationRequestBody": {
        "title": "Taxi Location And Status Request Body Schema",
        "required": [
          "apiKey",
          "lat",
          "lng",
          "status",
          "update_type",
          "user_type",
          "vehicle_num"
        ],
        "type": "object",
        "properties": {
          "apiKey": {
            "type": "string"
          },
          "vehicle_num": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "ACTIVE",
              "BOOKED",
              "INACTIVE"
            ]
          },
          "lat": {
            "type": "number"
          },
          "lng": {
            "type": "number"
          },
          "user_type": {
            "type": "string",
            "enum": [
              "Taxi"
            ]
          },
          "update_type": {
            "type": "string",
            "enum": [
              "taxiLoc"
            ]
          }
        }
      },
      "NewPassengerDetails": {
        "title": "New Passenger Details Schema",
        "required": [
          "firstname",
          "lastname",
          "password",
          "user_type",
          "username"
        ],
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string"
          },
          "lastname": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "user_type": {
            "type": "string",
            "enum": [
              "Passenger",
              "Taxi"
            ]
          }
        },
        "additionalProperties": false
      },
      "GetRideLocationRequestBody": {
        "title": "Get Ride Location Request Body Schema",
        "required": [
          "otpHash",
          "vehicle_num"
        ],
        "type": "object",
        "properties": {
          "otpHash": {
            "type": "string"
          },
          "vehicle_num": {
            "type": "string"
          }
        }
      },
      "Empty": {
        "title": "Empty Schema",
        "type": "object"
      },
      "GetPassengerUserResponseBody": {
        "title": "Get Passenger User Response Body Schema",
        "required": [
          "firstname",
          "lastname"
        ],
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string"
          },
          "lastname": {
            "type": "string"
          }
        }
      },
      "NewUserRequestBody": {
        "title": "New User Request Body Schema",
        "oneOf": [
          {
            "$ref": "#/components/schemas/NewTaxiDetails"
          },
          {
            "$ref": "#/components/schemas/NewPassengerDetails"
          }
        ]
